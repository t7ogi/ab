function PlayerChatted(Message)
	if ScriptDisabled then return end
	Split = Message:split(" ")
	Arg1 = Split[1]
	Arg2 = Split[2]
	Arg3 = Split[3]
	Arg4 = Split[4]

	if Command("unload") or Command("destroygui") or PrefixCommand("unload") or PrefixCommand("destroygui") then
		Notify("Unloading...", Color3.fromRGB(0, 255, 255), "Unloads")
		wait(.1)
		pcall(function()
			CmdGui:Destroy()
			States = {}
			LoopKill = {}
			LoopTase = {}
			Admin = {}
			ScriptDisabled = true
			for i,v in pairs(game.Lighting:GetChildren()) do
				v.Parent = workspace
			end
		end)
	end
	if Command("test") or PrefixCommand("test") then
		Notify("this is test, it worked!", Color3.fromRGB(255, 255, 255), "System")
	end
	if Command("reload") or Command("update") or PrefixCommand("reload") or PrefixCommand("update") then
		Notify("Updating versions...", Color3.fromRGB(255, 255, 255), "System")
		pcall(function()
			CmdGui:Destroy()
		end)
		Loadstring("https://pastebin.com/raw/xnJMa5U2")
	end
	if Command("keyboard") or Command("keyboard") then
		Notify("Phone keyboard executed", Color3.fromRGB(60, 60, 60), "Script")
		pcall(function()
			loadstring(game:HttpGet(('https://raw.githubusercontent.com/manimcool21/Keyboard-FE/main/Protected%20(3).lua'),true))()
		end)
	end
	if Command("tptool") or Command("clicktp") then
		Notify("Tp tool activated", Color3.fromRGB(0, 255, 0), "Success")
		pcall(function()
			mouse = game.Players.LocalPlayer:GetMouse()
			tool = Instance.new("Tool")
			tool.RequiresHandle = false
			tool.Name = "TP tool"
			tool.Activated:connect(function()
				local pos = mouse.Hit+Vector3.new(0,2.5,0)
				pos = CFrame.new(pos.X,pos.Y,pos.Z)
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
			end)
			tool.Parent = game.Players.LocalPlayer.Backpack
		end)
	end
	if Command("yield") or Command("infiniteyield") then
		Notify("Infinite Yield executed", Color3.fromRGB(60, 60, 60),"yield")
		pcall(function()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
		end)
	end
	if Command("fates admin") or Command("fates") then
		Notify("Fates admin executed", Color3.fromRGB(100, 100, 100), "Fates")
		pcall(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/fatesc/fates-admin/main/main.lua"))();
		end)
	end
	if Command("f3x") then
		Notify("Got f3x tool", Color3.fromRGB(100, 100, 100), "Client")
