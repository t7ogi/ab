		pcall(function()
			loadstring(game:HttpGet(('https://raw.githubusercontent.com/mgamingpro/HomebrewAdmin/master/Main'),true))()
		end)
	end
	if Command("fe") or Command("FE HUB") or Command("FE") or Command("fe hub") then
		Notify("FE hub executed", Color3.fromRGB(60, 60, 60), "FE hub")
		pcall(function()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/Dvrknvss/UniversalGames/main/UG'))()
		end)
	end
	if Command("dex") or Command("dexexplorer") or PrefixCommand("dex") then
		Notify("Dex explorer executed", Color3.fromRGB(255, 255, 255), "DEX")
		pcall(function()
			loadstring(game:HttpGet("https://pastebin.com/raw/drcXR8e2"))()
		end)
	end
	if Command("telekinesis") or Command("tel") or Command("teltool") then
		Notify("Got telekinesis tool", Color3.fromRGB(0, 255, 0), "Success")
		pcall(function()
			loadstring(game:HttpGet("https://pastebin.com/raw/uCUpjS7n"))()
		end)
	end
	if Command("help") then
		ChatNotify("Help : Write commands in the execute bar to execute them, you can search commands in the command searcher. If you accidently close the commands list you can use .cmds/.cmd to open it again, if for some reason nothing of this works use command .reload or .update, you can check what i added in the latest update using .logs/latestlogs command, if you need specific help in something dm me or use command .how/.howto.", Color3.fromRGB(255, 255, 0))
