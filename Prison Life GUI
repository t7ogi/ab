	if Command("howgunmod") then
		ChatNotify("There are some gun mod commands available, this commands changes values of your current gun like the reload time the burst of bullets and the max ammo, there are also some commands that automatically does it like autorapid fire or opgun, you can also change the type of fire of the gun like, smi automatic or automatic.", Color3.fromRGB(0, 255, 0))
		pcall(function()
		end)
	end
	if Command("howprefix") then
		ChatNotify("This script has some settings/functions related to the prefix, the prefix is the key that redirects you to the execute bar without the need of clicking it, the prefix is changable with the .prefix command and its also used if you try to use commands in the chat, there are some important commands that have a special prefix (!) this commands are GUI commands, commands wich function is to show important parts of the script, there is also a prefix to hide all the gui (Toggable) and you can change it with .guiprefix command.", Color3.fromRGB(0, 255, 0))
		pcall(function()
		end)
	end
	if Command("novoid") or Command("antivoid") then
		Notify("Antivoid turned on", Color3.fromRGB(0, 255, 0),"Succes")
		pcall(function()
			local lp = game.Players.LocalPlayer
			local chr = lp.Character
			local hrp = chr.HumanoidRootPart
			local cf = hrp.CFrame

			game.workspace.FallenPartsDestroyHeight = 0/0
			hrp.CFrame = hrp.CFrame * CFrame.new(0, -10000, 0)

			wait(1)

			hrp.CFrame = cf
			game.workspace.FallenPartsDestroyHeight = -200

			wait(1)

			game.workspace.FallenPartsDestroyHeight = 0/0
		end)
